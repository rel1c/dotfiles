#Alias for this file!
alias basha='vim ~/.bash_aliases'
alias bashs='source ~/.bashrc'

#Listing
alias ls='ls --color'
alias la='ls -a'
alias ll='la -lh'
alias l='ls'

#File navigation
alias cd..='cd ..'

#Some useful one letter aliases
alias q='exit'

#Personal log file
alias log='vim ~/documents/logs/log.txt'

#Personal blog file
alias blog='vim ~/documents/logs/blog.txt'
