#Alias for this file!
alias basha='vim ~/.bash_aliases'
alias bashs='source ~/.bashrc'

#Listing
alias l='ls --color'
alias la='ls -a --color'
alias ll='ls -al --color'

#File navigation
alias cd..='cd ..'

#Some useful one letter aliases
alias q='exit'

#Variables
alias null='/dev/null'

#Git
alias init='git init; touch "README.md"; touch ".gitignore"; git add *; git commit -m "init :tada:"'
alias gita='git add .'
alias gitc='git commit'
alias gith='git log --oneline'
alias gitl='git branch --list'
alias gitp='git push'
alias gits='git status'
alias gity='git pull'

#Common Files
alias log='vim ~/Documents/logs/log.txt'
